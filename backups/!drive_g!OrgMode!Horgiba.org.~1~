** Macro VBA
*** Supression lignes vides
Sub SuppressionVides()
' se base uniquement sur la première cellule
    Dim i, j As Integer
    i = 1
    j = 1
' j indique le nombre de ligne à tester
' en comptant les lignes vides
    While j < 100
        Cells(i, 1).Select
        If IsEmpty(Selection) Then
        Rows(i).EntireRow.Delete
        Else
        i = i + 1
        End If
        j = j + 1
    Wend
End Sub 
*** Sélection ligne par ligne
Sub LigneSelect()
    Selection.Offset(1, 0).Select
End Sub
*** Colorer case déscendre
Sub Macrocouleur()
    With Selection.Interior
        .Pattern = xlSolid
        .PatternColorIndex = xlAutomatic
        .Color = 65535
        .TintAndShade = 0
        .PatternTintAndShade = 0
    End With
   Selection.Offset(1, 0).Select
End Sub
*** Comapraison entre deux listes
Sub PauloVSRoHS()
    Dim i As Integer
    i = 2
    'ici c'est le nombre de lignes de la première liste
    While i < 69
    'on sélectionne l'onglet de la première liste
        Sheets("ListePauloTriee").Select
        Dim Ref, j As Integer
        Dim OK As Boolean
        OK = 0
        j = 2
        Ref = Cells(i, 1)
    'on sélectionne l'onglet de la seconde liste 
        Sheets("RoHS_HOR").Select
            While j < 189
                Cells(j, 4).Select
                
                If (Cells(j, 4).Value = Ref) Then
                OK = 1
                j = 189
                Else: j = j + 1
                End If
            Wend
            
        'on revient sur l'onglet de la première liste
        Sheets("ListePauloTriee").Select
        If OK Then
        Cells(i, 9).Value = "OK"
        Else
        Cells(i, 9).Value = "NOK"
        End If
        i = i + 1
    Wend
        
End Sub
** Macros VBA Spécifiques
*** Comparaison entre les deux analyses de risques
Sub AncienVSNouveau()
    Dim i As Integer
    i = 3
    'ici c'est le nombre de lignes de la première liste
    While i < 195
    'on sélectionne l'onglet de la première liste
        Sheets("Analyse de risques").Select
        Dim j, L As Integer
        Dim AncRef, NouvRef As String
        Dim OK As Boolean
        OK = 0
        j = 7
        AncRef = Cells(i, 1).Value
        L = Len(AncRef)
        NouvRef = ""
    'on sélectionne l'onglet de la seconde liste
        Sheets("Comparaison").Select
            While j < 77
                Cells(j, 4).Select
                NouvRef = Cells(j, 4).Value
                If (AncRef = Right(Cells(j, 4), L)) Then
                OK = 1
                NouvRef = Cells(j, 2).Value
                j = 189
                Else: j = j + 1
                End If
            Wend
            
        'on revient sur l'onglet de la première liste
        Sheets("Analyse de risques").Select
        If OK Then
        Cells(i, 14).Value = NouvRef
        Else
        Cells(i, 14).Value = "NOK"
        End If
        i = i + 1
    Wend
        
End Sub
